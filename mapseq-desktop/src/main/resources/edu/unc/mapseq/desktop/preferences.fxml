<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="160" prefWidth="300" stylesheets="@preferences.css" xmlns="http://javafx.com/javafx/8"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.unc.mapseq.desktop.PreferencesController">
  <children>
    <GridPane alignment="CENTER" hgap="10" vgap="10">
      <padding>
        <Insets top="25" right="25" bottom="10" left="25" />
      </padding>
      <Label text="Server:" GridPane.rowIndex="0" GridPane.columnIndex="0" />
      <TextField fx:id="server" GridPane.rowIndex="0" GridPane.columnIndex="1" />
      <Label text="Port:" GridPane.rowIndex="1" GridPane.columnIndex="0" />
      <TextField fx:id="port" GridPane.rowIndex="1" GridPane.columnIndex="1" />
      <HBox spacing="10" alignment="CENTER" GridPane.columnSpan="2" GridPane.columnIndex="0"
        GridPane.rowIndex="4">
        <Button text="OK" onAction="#handleOK" />
        <Button text="Cancel" onAction="#handleCancel" />
      </HBox>
    </GridPane>
  </children>
</AnchorPane>
